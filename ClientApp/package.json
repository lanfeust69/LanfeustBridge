{
  "name": "lanfeust-bridge",
  "version": "0.0.0",
  "description": "The angular web app part of LanfeustBridge",
  "license": "MIT",
  "repository": "github:lanfeust69/LanfeustBridge",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "test": "ng test",
    "test-ci": "ng test --code-coverage --no-watch --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e:backend": "cd .. && dotnet run --launch-profile BackendForE2E",
    "e2e:run": "ng e2e",
    "e2e:run-headless": "ng e2e --protractor-config protractor-headless.conf.js",
    "e2e": "run-p --race e2e:backend e2e:run",
    "e2e-ci": "run-p --race e2e:backend e2e:run-headless"
  },
  "dependencies": {
    "@angular/animations": "7.2.8",
    "@angular/common": "7.2.8",
    "@angular/compiler": "7.2.8",
    "@angular/core": "7.2.8",
    "@angular/forms": "7.2.8",
    "@angular/http": "7.2.8",
    "@angular/platform-browser": "7.2.8",
    "@angular/platform-browser-dynamic": "7.2.8",
    "@angular/platform-server": "7.2.8",
    "@angular/router": "7.2.8",
    "@aspnet/signalr": "^1.1.2",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.5",
    "@angular/cli": "7.3.5",
    "@angular/compiler-cli": "7.2.8",
    "@angular/language-service": "7.2.8",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.26",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "typescript": "3.2.4"
  }
}
