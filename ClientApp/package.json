{
  "name": "lanfeust-bridge",
  "version": "0.0.0",
  "description": "The angular web app part of LanfeustBridge",
  "license": "MIT",
  "repository": "github:lanfeust69/LanfeustBridge",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr:dev": "ng run lanfeust-bridge:server:dev",
    "build:ssr:prod": "ng run lanfeust-bridge:server:production",
    "test": "ng test",
    "test-ci": "ng test --code-coverage --no-watch --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e:backend": "cd .. && dotnet run --launch-profile BackendForE2E",
    "e2e:run": "ng e2e",
    "e2e:run-headless": "ng e2e --protractor-config protractor-headless.conf.js",
    "e2e": "run-p --race e2e:backend e2e:run",
    "e2e-ci": "run-p --race e2e:backend e2e:run-headless"
  },
  "dependencies": {
    "@angular/animations": "8.1.2",
    "@angular/common": "8.1.2",
    "@angular/compiler": "8.1.2",
    "@angular/core": "8.1.2",
    "@angular/forms": "8.1.2",
    "@angular/platform-browser": "8.1.2",
    "@angular/platform-browser-dynamic": "8.1.2",
    "@angular/platform-server": "8.1.2",
    "@angular/router": "8.1.2",
    "@aspnet/signalr": "^1.1.4",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "8.1.2",
    "@angular/compiler-cli": "8.1.2",
    "@angular/language-service": "8.1.2",
    "@types/jasmine": "^3.3.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.6.8",
    "codelyzer": "^5.0.1",
    "jasmine-core": "^3.4.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "3.4.5"
  }
}
